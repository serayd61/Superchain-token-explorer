# Superchain Token Explorer Environment Variables
# Copy this file to .env.local and update with your values

# RPC URLs (Optional - defaults provided)
# You can get better RPC endpoints from:
# - Alchemy: https://www.alchemy.com/
# - QuickNode: https://www.quicknode.com/
# - Infura: https://infura.io/

# OP Stack Chains
BASE_RPC_URL=https://mainnet.base.org
OPTIMISM_RPC_URL=https://mainnet.optimism.io
MODE_RPC_URL=https://mainnet.mode.network
ZORA_RPC_URL=https://rpc.zora.energy
FRAXTAL_RPC_URL=https://rpc.frax.com
WORLD_RPC_URL=https://worldchain-mainnet.g.alchemy.com/public
LISK_RPC_URL=https://rpc.api.lisk.com

# Other Chains
ETHEREUM_RPC_URL=https://ethereum.publicnode.com
ARBITRUM_RPC_URL=https://arbitrum.publicnode.com
POLYGON_RPC_URL=https://polygon.llamarpc.com

# API Keys (Optional)
# Get your API key from: https://dexscreener.com/
DEXSCREENER_API_KEY=

# Etherscan API Keys (for contract verification)
ETHERSCAN_API_KEY=
BASESCAN_API_KEY=
OPTIMISM_ETHERSCAN_API_KEY=

# Analytics (Optional)
NEXT_PUBLIC_GA_ID=
NEXT_PUBLIC_POSTHOG_KEY=

# Production URL (update after deployment)
NEXT_PUBLIC_APP_URL=http://localhost:3000